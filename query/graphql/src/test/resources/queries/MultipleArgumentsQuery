query TestMultipleArgs($stringArg: String, $integerArg: Int, $longArg: Long, $booleanArg: Boolean, $listArg: [String], $enumArg: Enumeration) {
  testing {
    multipleArgs(string: $stringArg, integer: $integerArg, long: $longArg, boolean:$booleanArg, list: $listArg, enumeration: $enumArg) {
      string
      boolean
      integer
      long
      list
      enumeration
    }
  }
}